Include ~/.orbstack/ssh/config
Include config.d/*

Host github.com
  IdentityAgent none

Host gitlab.com
  User git

Host homeassistant.local
  User root
  Port 2222

Host 192.168.1.70
  User root
  Port 2222

Host *
  User ubuntu
  StrictHostKeyChecking no
  ServerAliveInterval 30
  ForwardAgent yes
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa

